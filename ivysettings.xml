<ivysettings>
<settings defaultResolver="defaultresolver" />
<property name="ibiblio-maven2-root" value="http://172.31.86.246:8082/artifactory/m2-releases/" />
<property name="m2snapshots" value="http://172.31.86.246:8082/artifactory/m2-snapshots/" />
<property name="m2releases" value="http://172.31.86.246:8082/artifactory/m2-releases/" />
<credentials host="172.31.86.246" realm="Artifactory Realm" username="${jFrogUsername}" passwd="${jFrogPassword}"/>
<resolvers>
    <chain name="defaultresolver">
    <ibiblio name="mavenroot" m2compatible="true" root="${m2}" />
    <ibiblio name="m2snapshots" m2compatible="true" root="${m2-snapshots}" />
    <ibiblio name="m2sreleases" m2compatible="true" root="${m2-releases}" />
        <url name="repository">
           <ivy pattern="${m2-snapshots}/[organisation]/[module]/[revision]/ivy-[revision].xml" />
           <artifact pattern="${m2-snapshots}/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
        </url>   
    </chain>
</resolvers>
</ivysettings>
